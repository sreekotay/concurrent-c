#include <ccc/std/prelude.cch>

int main(void) {
    @arena {
        CCString s = cc_string_new(arena);
        s.append("nested\n");

        // Outer UFCS call spans multiple lines and contains an inner UFCS call.
        cc_std_out.write(
            s.as_slice()
        );
    }
    return 0;
}


