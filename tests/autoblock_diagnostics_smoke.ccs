#include "cc_runtime.cch"

extern int ext_blocking(int x);

@async void f(int x) {
    /* In the future this should be auto-wrapped; for now we only emit a note when CC_DEBUG_AUTOBLOCK=1. */
    (void)x;
    ext_blocking(1);
}

int ext_blocking(int x) { return x + 1; }

int main(void) {
    // NOTE: @async semantics are not implemented; this just ensures parsing + stripping works.
    f(1);
    return 0;
}

