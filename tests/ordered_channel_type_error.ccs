// Test that ordered_recv on regular channel fails type check
#include <ccc/std/prelude.cch>

int main() {
    int[~4 >] tx;
    int[~4 <] rx;  // Regular (not ordered) channel
    channel_pair(&tx, &rx);
    
    void* result;
    // This should fail: ordered_recv expects CCChanRxOrdered, not CCChanRx
    ordered_recv(rx, &result);
    
    return 0;
}
